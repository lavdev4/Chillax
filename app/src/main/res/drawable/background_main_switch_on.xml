<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <size android:width="50dp" android:height="50dp"/>
            <corners android:radius="8dp"/>
            <solid android:color="?attr/colorPrimary"/>
        </shape>
    </item>

    <item
        android:top="3dp"
        android:left="3dp"
        android:right="3dp"
        android:bottom="3dp">
        <shape>
            <corners android:radius="5dp"/>
            <stroke android:width="1dp" android:color="#996200EE"/>
        </shape>
    </item>

    <item android:drawable="@drawable/ic_main_switch_on"
        android:top="6dp"
        android:left="6dp"
        android:right="6dp"
        android:bottom="6dp"/>
</layer-list>